<?xml version="1.0" encoding="UTF-8" ?>
<gresources>
  <gresource prefix="/io/github/bracket">
    <file
      compressed="true"
      preprocess="xml-stripblanks"
      alias="window.ui"
    >ui/window.ui</file>
    <file
      compressed="true"
      preprocess="xml-stripblanks"
      alias="tabview.ui"
    >ui/tabview.ui</file>
    <file
      compressed="true"
      preprocess="xml-stripblanks"
      alias="toolbar.ui"
    >ui/toolbar.ui</file>
    <file
      compressed="true"
      preprocess="xml-stripblanks"
      alias="editor.ui"
    >ui/editor.ui</file>
    <file
      compressed="true"
      preprocess="xml-stripblanks"
      alias="directory_browser.ui"
    >ui/directory_browser.ui</file>
  </gresource>
  <gresource prefix="/styles">
    <file alias="style.css" compressed="true">styles/style.css</file>
  </gresource>
</gresources>
